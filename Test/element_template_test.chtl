[Template] @Element SimpleCard {
    div {
        class: "card";
        h2 {
            class: "card-title";
            text: "Card Title";
        }
        p {
            class: "card-body";
            text: "This is the body of the card.";
        }
    }
}

html {
    body {
        h1 {
            text: "Testing Element Templates";
        }

        @Element SimpleCard;
    }
}