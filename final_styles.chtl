// This file will be imported into the 'lib' namespace
[Namespace] lib

[Template] @Var Sizing {
    base-unit: 8px;
}

[Template] @Style Button {
    // Note: This variable will resolve to the 'lib' namespace's 'Sizing' group
    // because the parser's context will be in the 'lib' namespace when this is parsed.
    padding: Sizing(base-unit);
    font-size: 16px;
}