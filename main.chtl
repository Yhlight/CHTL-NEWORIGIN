// Global scope variable
[Template] @Var Theme {
    text-color: black;
}

[Namespace] MyTheme

// This is now in the MyTheme namespace
[Template] @Var Theme {
    text-color: blue;
}

body {
    p {
        // This should resolve to the 'MyTheme' namespace version
        style {
            color: Theme(text-color);
        }
        text { "This text should be blue." }
    }
}
